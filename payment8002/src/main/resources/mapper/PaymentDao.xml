<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.yuhao.springcloud.payment.dao.PaymentDao">
    <resultMap id="paymentResultMap" type="org.yuhao.springcloud.payment.model.po.PaymentPo">
        <id column="id" property="id"/>
        <id column="serial" property="serial"/>
    </resultMap>

    <insert id="create" useGeneratedKeys="true" keyProperty="id">
        insert into payment (serial) values (#{serial})
    </insert>

    <select id="get" resultMap="paymentResultMap">
        select * from payment where id = #{id}
    </select>
</mapper>